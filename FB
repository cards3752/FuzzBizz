


//Node.js 6.5
//Plain Javascript and Node.js is supported
// html/css is not supported 

for (let current = 1; current < 101; current++){
	if (current % 15 == 0){
		console.log("FizzBuzz");
	}
	else if (current % 5 == 0){
		console.log("Buzz");
	}
	else if (current % 3 == 0){
		console.log("Fizz");
	}
	else console.log(current);
}


